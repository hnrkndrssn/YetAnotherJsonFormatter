{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "letsencryptproxy": {
            "matchCondition": {
                "route": "/.well-known/acme-challenge/{code}"
            },
            "backendUri": "http://%WEBSITE_HOSTNAME%/letsencrypt/{code}"
        }
    }
}